{
  "mcpServers": {
    "firecrawl": {
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": {
        "FIRECRAWL_API_KEY": ""
      },
      "disabled": false,
      "autoApprove": []
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp"
      ],
      "disabled": false,
      "autoApprove": [
        "fill_form",
        "click",
        "wait_for",
        "take_snapshot"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": ""
      },
      "disabled": false,
      "autoApprove": []
    },
    "supabase": {
      "url": "https://mcp.supabase.com/mcp",
      "headers": {
        "Authorization": ""
      },
      "disabled": false
    },
    "vercel": {
      "url": "https://mcp.vercel.com",
      "headers": {
        "Authorization": ""
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}
